import java.util.*;
public class perfect{
public static void main(String args[]){
	int n;
	Scanner obj=new Scanner(System.in);
	System.out.print("enter the no of elements : ");
	n=obj.nextInt();
	for(int i=1;i<=100000 && n>0;i++){
		int c=0;
		for(int j=1;j<i;j++){
			if(i%j==0){
			c+=j;
		}
	}
	if(c==i){
		System.out.println(i);
	if(n==i) break;
		else n--;
	}
	}
	}
}
